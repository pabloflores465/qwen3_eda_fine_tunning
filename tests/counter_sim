#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x100a3cb10 .scope module, "tb_counter4bit" "tb_counter4bit" 2 3;
 .timescale -9 -12;
v0x100a446c0_0 .var "clk", 0 0;
v0x100a44760_0 .net "count", 3 0, v0x100a404e0_0;  1 drivers
v0x100a44800_0 .var "rst", 0 0;
S_0x100a402c0 .scope module, "dut" "counter4bit" 2 10, 3 1 0, S_0x100a3cb10;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v0x100a40440_0 .net "clk", 0 0, v0x100a446c0_0;  1 drivers
v0x100a404e0_0 .var "count", 3 0;
v0x100a44620_0 .net "rst", 0 0, v0x100a44800_0;  1 drivers
E_0x100a443a0 .event posedge, v0x100a44620_0, v0x100a40440_0;
    .scope S_0x100a402c0;
T_0 ;
    %wait E_0x100a443a0;
    %load/vec4 v0x100a44620_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x100a404e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x100a404e0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x100a404e0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x100a3cb10;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x100a446c0_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x100a446c0_0;
    %inv;
    %store/vec4 v0x100a446c0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x100a3cb10;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x100a44800_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x100a44800_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x100a44800_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x100a44800_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 36 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x100a3cb10;
T_3 ;
    %vpi_call 2 41 "$display", "Time   rst  count" {0 0 0};
    %vpi_call 2 42 "$monitor", "%4t   %b    %b", $time, v0x100a44800_0, v0x100a44760_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x100a3cb10;
T_4 ;
    %vpi_call 2 47 "$dumpfile", "counter4bit.vcd" {0 0 0};
    %vpi_call 2 48 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x100a3cb10 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_counter4bit.v";
    "counter4bit.v";
